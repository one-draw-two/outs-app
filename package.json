{
  "name": "outs-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:s": "pnpm build && NUXT_PUBLIC_PLATFORM=cap pnpm generate && pnpm cap:sync",
    "build:a": "npx nuxi analyze",
    "build:sw": "NODE_ENV=production VITE_SW_DEBUG=true VITE_DISABLE_HMR=true nuxt generate && concurrently \"npx serve -l tcp://127.0.0.1:3000 .output/public\" \"npx localtunnel --port 3000 --subdomain outs-sw\"",
    "dev": "DEV=true NODE_ENV=development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "cap:android:sync": "npx cap sync android",
    "cap:android:open": "npx cap open android",
    "cap:android:icons": "npx cordova-res android --skip-config --copy",
    "cap:ios:sync": "npx cap sync ios",
    "cap:ios:open": "npx cap open ios",
    "cap:ios:icons": "npx cordova-res ios --skip-config --copy",
    "cap:icons": "npx cordova-res --skip-config --copy",
    "cap:sync": "npx cap sync android && npx cap sync ios"
  },
  "dependencies": {
    "@capacitor-community/fcm": "^7.1.1",
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.2",
    "@hugeicons/core-free-icons": "^1.0.16",
    "@hugeicons/vue": "^1.0.3",
    "@outs1x2/capacitor-liveactivities": "^0.2.6",
    "@powersync/web": "^1.25.1",
    "@tailwindcss/vite": "^4.1.12",
    "@tailwindplus/elements": "^1.0.9",
    "capacitor-plugin-safe-area": "^4.0.0",
    "dayjs": "^1.11.13",
    "jwt-decode": "^4.0.0",
    "nuxt": "^4.0.3",
    "tailwindcss": "^4.1.12",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "concurrently": "^9.2.0",
    "localtunnel": "^2.0.2",
    "typescript": "^5.9.2",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  }
}
