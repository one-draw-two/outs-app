{
  "name": "outs-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:s": "pnpm build && NUXT_PUBLIC_PLATFORM=cap pnpm generate && pnpm cap:sync",
    "build:a": "npx nuxi analyze",
    "dev": "DEV=true NODE_ENV=development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "cap:android:sync": "npx cap sync android",
    "cap:android:open": "npx cap open android",
    "cap:android:icons": "npx cordova-res android --skip-config --copy",
    "cap:ios:sync": "npx cap sync ios",
    "cap:ios:open": "npx cap open ios",
    "cap:ios:icons": "npx cordova-res ios --skip-config --copy",
    "cap:sync": "npx cap sync android && npx cap sync ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/background-runner": "^2.1.0",
    "@capacitor/core": "^7.0.1",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/preferences": "^7.0.0",
    "@powersync/web": "^1.14.1",
    "@tailwindcss/vite": "^4.0.12",
    "dayjs": "^1.11.13",
    "nuxt": "^3.15.4",
    "tailwindcss": "^4.0.12",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.1",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
